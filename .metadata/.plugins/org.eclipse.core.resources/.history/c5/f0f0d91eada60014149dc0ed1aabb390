package com.wgh;

public class LoginAction {
	private String username;

	public void setUsername(String username) {
		this.username = username;
	}

	public String getUsername() {
		return username;
	}
	
	public String excute() throws Exception{
		if(getUsername().equals("www")){
			return "success";
		}else{
			return "error";
		}
	}

}
